@model  TicketShopProject.ViewModels.HomeViewModel
@await Html.PartialAsync("Carousel")

<h2>Prefered Tickets  of the week </h2>
<h4>Clients preferences - just for you!</h4>
<div class="row">
    @foreach (var ticket  in Model.PreferredTickets)
    {
        @Html.Partial("TicketSummary", ticket )
    }
</div>